<% form_for @participant_session, :url => participant_session_path do |f| %>
  <%= f.error_messages %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>  
  <div>
    <%= f.label :password %><%#= link_to('forgot?', new_password_reset_path) -%>
    <%= f.password_field :password %>
  </div>  
  <div>
    <%= f.check_box :remember_me %>
    <%= f.label :remember_me %>
  </div>
  <div>
    <%= f.submit "Sign In" %>
  </div>
<% end %>
